{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/trading",
      "dest": "/index.js"
    },
    {
      "src": "/analytics/(.*)",
      "dest": "/index.js"
    }
  ]
}
